{
  "name": "delgate",
  "version": "1.0.0",
  "description": "Delgate main screen",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "prefix:css": "postcss --use autoprefixer -b css/style.css -o css/style.prefix.css",
    "start": "npm-run-all --parallel devserver watch:sass"
  },
  "author": "Ketan",
  "license": "ISC",
  "devDependencies": {
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3"
  }
}
